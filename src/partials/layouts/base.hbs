<!doctype html>
<html lang="ru">
<head>
    {{#> head-block}}
      {{!-- <head> content per page. --}}
    {{/head-block}}
</head>
<body>
  <div id="pt-main" class="pt-perspective">
    <div class="pt-page-0 pt-page">
      {{#> header}}
      {{!-- Header - block --}}
      {{/header}}

      {{#> content-block}}
      {{!-- Content inserts --}}
      {{/content-block}}


      {{#> footer-block}}
      {{!--
        The `includes/footer` partial is the default content,
        but can be overridden.
      --}}
      {{/footer-block}}
      {{#> scripts-block}}
      {{!-- Custom scripts per page can be added. --}}
      {{/scripts-block}}
    </div>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <!-- Plugins -->
  </div>
  {{> layouts/common-scripts}}
  {{> layouts/yandex-metrika}}
</body>
</html>
